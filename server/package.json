{
  "name": "contrib-server",
  "version": "1.0.0",
  "description": "Boilerplate generated by express-generator-node-typescript-apollo-graphql-mongo",
  "main": "dist/index.js",
  "license": "ISC",
  "scripts": {
    "start": "NODE_ENV=local nodemon --exec ts-node ./index.ts",
    "build": "tsc --build tsconfig.build.json",
    "format": "prettier --write \"./**/*.ts\"",
    "lint": "eslint \"./**/*.ts\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests"
  },
  "dependencies": {
    "@newrelic/winston-enricher": "^1.0.0",
    "@types/fs-extra": "^9.0.7",
    "apollo-server-express": "^2.21.0",
    "asoiaf-api": "^1.0.1",
    "auth0": "^2.33.0",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.1.0",
    "graphql": "^14.3.0",
    "mongoose": "^5.8.2",
    "newrelic": "^7.1.1",
    "twilio": "^3.56.0",
    "typescript": "^4.0.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/auth0": "^2.33.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.20",
    "@types/mongoose": "^5.8.2",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^1.19.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "ts-node": "^8.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
